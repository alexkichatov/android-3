<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape >
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
    <item android:top="1dp">
        <shape >
            <solid android:color="#B9C7D5" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:top="1dp">
        <selector >
            <item android:state_pressed="true">
                <shape >
                    <solid android:color="#D1D3D9" />
                </shape>
            </item>
            <item>
                <shape >
                    <solid android:color="#EFF2F9" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>